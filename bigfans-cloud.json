                                        "b2": {
                                            "type": "Array", // 表示是一个数组，数组中的类型是Integer
                                            "name": "Integer"
                                        },
                                        "c": {
                                            "type": "Basic",
                                            "name": "Float" // 浮点数，不区分精确度，包括 float或者double等
