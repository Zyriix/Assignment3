                                                },
                                                "items": {
                                                    "type": "Array",
                                                    "name": "OrderCreateItemForm"
                                                }
                                            }
                                        },
                                        "a2": {
                                            "type": "Array",
                                            "struct": {
                                                "b": {
                                                    "type": "Basic",
                                                    "name": "Integer"
                                                }
                                            }
                                        },
                                        "b": {
                                            "type": "Basic",
                                            "name": "Integer" // 整数，不区分字节长度，包括了 int或者long等
